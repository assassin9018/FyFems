             xmlns:local="clr-namespace:MyFemsWpfClient"
             Startup="OnStartup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles\_Colors.xaml" />
